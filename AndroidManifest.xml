<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rsalesarm.events">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sign_in" />
        <activity android:name=".MyProfile" />
        <activity android:name=".CreateEvent" />
        <activity android:name=".Events"></activity>
        <provider
            android:authorities="com.example.rsalesarm.events"
            android:name=".data.EventProvider"
            android:exported="false"/>
    </application>

</manifest>